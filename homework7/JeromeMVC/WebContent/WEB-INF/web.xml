<?xml version="1.0" encoding="UTF-8"?>
<web-app>
	<!-- 配置拦截器所有的请求转成index -->
	<servlet>
		<servlet-name>DispatcherServlet</servlet-name>
		<servlet-class>servlet.DispatcherServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>DispatcherServlet</servlet-name>
		<url-pattern>/</url-pattern>
	</servlet-mapping>
</web-app>